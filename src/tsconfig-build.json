{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": false,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../build",
    "rootDir": ".",
    "rootDirs": [
      "."
    ],
    "sourceMap": true,
    "inlineSources": true,
    "target": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@ray-volution/*": [
        "./*"
      ]
    }
  },
  "files": [
    "public_api.ts"
  ]
}
